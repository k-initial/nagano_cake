<%= form_with model: @genre, url: admin_genres_path do |f| %>
<%= f.label :name, "ジャンル名" %>
<%= f.text_field :name %>
<%= f.submit "新規登録",class: "btn" %>
<% end %>

<h2>ジャンル</h2>
<% @genres.each do |genre| %>
<%= genre.name %>
<%= link_to "編集する", edit_admin_genre_path(genre), class: "btn" %>
<% end %>